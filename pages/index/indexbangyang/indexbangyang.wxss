.swiper-tab {
    width: 100%;
    text-align: center;
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    color: #ccc;
    font-size: 33rpx;
    background-color: #fff;
    padding: 0rpx 120rpx;
    margin-bottom: 10rpx;
}

.swiper-tab-item {
    width: 40%;
    color: #999;
    font-size: 34rpx;
    font-weight: bold;
}

.active {
    color: #c31926;
    border-bottom: 4rpx solid #c31926;
    font-size: 34rpx;
    font-weight: bold;
}

.juzhong {
    margin: 0 auto;
}

.domain {
    background-color: #fff;
    height: 100%;
    margin: 0 10px;
}

.show {
    display: block;
}

.hidden {
    display: none;
}

.active_item_title {
    padding: 15px;
    background-color: #fff;
}

.active_title_wrap {
    text-align: center;
    padding-bottom: 2px;
    border-bottom: 1px solid #000;
    width: 80px;
    margin: 0 auto;
}

.active_title {
    display: inline-block;
    font-size: 14px;
    padding: 0 10px;
    padding-bottom: 5px;
    border-bottom: 3px solid #000;
    font-weight: 700;
}

.active_goods_name {
    background-color: #fff;
    padding: 7px 10px;
    font-size: 14px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.active_item_infor {
    background-color: #fff;
    font-size: 14px;
    padding: 0 10px;
    padding-bottom: 10px;
}

.active_new_price {
    color: #f00;
}

.active_old_price {
    font-size: 12px;
    text-decoration: line-through;
    margin-left: 5px;
}

.active_time {
    float: right;
    font-size: 12px;
}

.active_block {
    margin-bottom: 10px;
}

.active_group_price {
    font-size: 12px;
    color: #f00;
}

.slide-image {
    height: 100%;
    width: 100%;
}

.category {
    margin: 0 12rpx;
    border-radius: 20rpx;
    position: relative;
    top: -10rpx;
    left: 0;
    box-shadow: 0rpx 0rpx 50rpx #fff;
    padding: 0;
    background-color: #f5f6f7;
    display: flex;
    justify-content: space-between;
    background: url(http://hm.zhuwentec.com/cul-cloud/wxapp/images/category_bg.png);
    background-size: cover;
}

.category_item {
    padding-top: 22rpx;
    padding-bottom: 26rpx;
    margin-bottom: 0;
}

.icon_wrap {
    width: 62rpx;
    margin: 0 auto;
    margin-bottom: 12rpx;
}

.index_icon {
    width: 62rpx;
    height: 62rpx;
}

.category_item_text {
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.category_item_text text {
    text-align: center;
}

.activeSelect .activeList:first-child {
    border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.activeSelect .activeList {
    width: 100%;
    background-color: #fff;
    margin: 1% 12rpx 8rpx 0;
    overflow: hidden;
    margin-top: 2rpx;
}

.activeSelect .activeList .item {
    margin: 3% 0 3% 0;
    background-color: #fff;
    border-bottom: 2rpx solid #f0f0f0;
    height: 300rpx;
}

.activeSelect .activeList .item:last-child {
    margin-bottom: 1%;
    border-bottom: none;
}

.activeSelect {
    margin: 1% 12rpx 0 12rpx;
    margin-top: 2rpx;
}

.head-inf {
    padding-left: 0;
    font-size: 30rpx;
    align-items: center;
    color: #000;
}

.head-inf .name {
    font-size: 34rpx;
    margin-right: 18rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.per-inf {
    display: flex;
    margin-top: 6rpx;
}

.per-inf .sex {
    font-size: 26rpx;
    margin-right: 70rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #a6a6a6;
}

.per-inf .age {
    font-size: 26rpx;
    color: #a6a6a6;
}

.activeSelect .activeList .item .info .time {
    font-size: 24rpx;
    color: #a6a6a6;
    margin-bottom: 20rpx;
    margin-top: 14rpx;
}

.activeSelect .activeList .item .info .addr {
    font-size: 26rpx;
    color: #666;
    margin-top: 2rpx;
    display: flex;
    justify-content: flex-start;
}

.activeSelect .activeList .item .info .addr view {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.activeSelect .activeList .item .info {
    position: relative;
    width: 66%;
    height: 242rpx;
    background-color: #fff;
    float: left;
    padding-left: 4%;
}

.activeSelect .activeList .item .info .options {
    width: 100%;
    position: absolute;
    bottom: -36rpx;
    left: 4%;
    display: flex;
    justify-content: space-between;
    padding-right: 15rpx;
}

.head {
    line-height: 50rpx;
}

.activeSelect .activeList .item .info .options .head text {
    background-color: #ffefef;
    color: #d10302;
    font-size: 26rpx;
    height: 30rpx;
    line-height: 30rpx;
    padding: 8rpx 20rpx;
    border-radius: 24rpx;
}

.activeSelect .activeList .item .info .options .price {
    float: left;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    background-color: #c31926;
    color: #fff;
    font-size: 28rpx;
    border-radius: 10rpx;
    padding-left: 20rpx;
    padding-right: 10rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.activeSelect .activeList .item .info .options .price image {
    width: 18rpx;
    height: 18rpx;
}

.activeSelect .activeList .item .img {
    margin-left: 2%;
    height: 280rpx;
    width: 210rpx;
    background-color: #fff;
    border-radius: 5%;
    float: left;
}

.area-name {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.activeSelect .activeList .item .img image {
    width: 100%;
    height: 100%;
    border-radius: 5%;
}