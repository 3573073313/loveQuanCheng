.wode-collect-wrapper {
    margin: 20rpx;
    padding: 0rpx 20rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.collect-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 0rpx;
    border-bottom: 1px solid #f5f5f5;
}

.collect-item-tit {
    font-size: 32rpx;
    margin-bottom: 24rpx;
}

.collect-time {
    font-size: 26rpx;
    color: #666;
    display: flex;
    justify-content: space-between;
}

.collect-item image {
    width: 100rpx;
    height: 100rpx;
    margin-right: 20rpx;
    border-radius: 10rpx;
}

.collect-type {
    color: #fff;
    border-radius: 10rpx;
    background-color: #FF7F00;
    padding: 0rpx 8rpx;
    height: 34rpx;
    line-height: 30rpx;
}