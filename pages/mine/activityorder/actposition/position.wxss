.nearby {
    height: 46rpx;
    background-color: #fff;
    padding-left: 26rpx;
    font-size: 26rpx;
    color: #333;
    line-height: 46rpx;
}

.activeSelect {
    margin: 4rpx 12rpx 0 12rpx;
    background-color: #FFF;
    overflow: hidden;
    border-radius: 10rpx 10rpx 0 0;
}

.activeSelect .activeList {
    width: 100%;
    background-color: #FFF;
    margin-top: 2%;
}

.activeSelect .activeList .item {
    margin: 2% 0 2% 0;
    background-color: #FFF;
    border-bottom: 4rpx solid #F0F0F0;
    padding-left: 2%;
    position: relative;
}

.activeSelect .activeList .item:last-child {
    border-bottom: none;
}

.activeSelect .activeList .item .img {
    width: 30%;
    height: 260rpx;
    background-color: #FFF;
    border-radius: 5%;
    float: left;
}

.activeSelect .activeList .item .img image {
    width: 100%;
    height: 100%;
    border-radius: 5%;
}

.activeSelect .activeList .item .info {
    width: 66%;
    height: 242rpx;
    background-color: #FFF;
    float: left;
    padding-left: 4%;
    position: relative;
}

.activeSelect .activeList .item .info .star {
    margin-top: 7rpx;
    overflow: hidden;
}

.star-img {
    width: 22rpx;
    height: 22rpx;
    margin-right: 10rpx;
}

.time-img image {
    max-width: 110%;
    width: 110%;
    height: 110%;
}

.activeSelect .activeList .item .info .time text {
    line-height: 36rpx;
}

.activeSelect .activeList .item .info .addr {
    font-size: 26rpx;
    color: #333;
    font-weight: normal;
    margin-top: 8rpx;
    display: flex;
    justify-content: flex-start;
}

.addr-img {
    width: 42rpx;
    height: 32rpx;
    padding-right: 10rpx;
}

.addr-img image {
    width: 100%;
    height: 100%;
}

.activeSelect .activeList .item .info .addr text {
    width: 80%;
    line-height: 40rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.activeSelect .activeList .item .info .options {
    width: 100%;
    height: 50rpx;
    position: absolute;
    bottom: -17rpx;
    left: 4%;
    display: flex;
    justify-content: space-between;
}

.activeSelect .activeList .item .info .options .head {
    width: 180rpx;
    height: 30rpx;
    line-height: 50rpx;
}

.activeSelect .activeList .item .info .options .head text {
    background-color: #E5E5E5;
    color: #000;
    font-size: 24rpx;
    height: 30rpx;
    line-height: 30rpx;
    padding: 4rpx 20rpx 4rpx 20rpx;
    border-radius: 11rpx;
}

.activeSelect .activeList .item .info .options .price {
    float: left;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    background-color: #C31926;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    border-radius: 10rpx;
}

.head-inf {
    padding-left: 0;
    font-weight: bold;
    font-size: 30rpx;
    width: 70%;
    overflow: hidden;
    display: flex;
}

.head-inf text {
    float: left;
    margin-right: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.head-inf .head2 {
    width: 24rpx;
    height: 24rpx;
    color: #fff;
    font-size: 16rpx;
    border-radius: 50%;
    background-color: #FF3A11;
    float: left;
    margin-top: 10rpx;
    text-align: center;
}

.online {
    margin-top: 11rpx;
    font-size: 22rpx;
    color: #666;
}

.activeList .distance {
    color: #666;
    font-size: 22rpx;
    position: absolute;
    top: 6rpx;
    right: 20rpx;
}

.btn-area {
    padding: 0 30rpx;
}

.search_btn {
    line-height: 80rpx;
}