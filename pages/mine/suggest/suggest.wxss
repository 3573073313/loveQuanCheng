.release-news-wrap {
    margin: 20rpx 20rpx 0 20rpx;
    border-radius: 10rpx;
    height: 100%;
}

.release-news-infos {
    padding: 0 22rpx 24rpx;
    font-size: 24rpx;
    color: #999;
    background-color: #fff;
    border-radius: 10rpx;
}

.release-news-infos .cont {
    padding-top: 24rpx;
    min-height: 350rpx;
    width: 100%;
    color: #333;
    font-size: 28rpx!important;
}

.release-news-infos .add-imgs {
    display: flex;
    flex-wrap: wrap;
}

.info4-imgs {
    width: 30%;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
    border-radius: 10rpx;
    position: relative;
}

.release-news-infos image {
    width: 100%;
    height: 100%;
}

.release-news-infos .index-top-tx {
    width: 200rpx;
    height: 190rpx;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
    border-radius: 10rpx;
}

.release-news-infos .del {
    position: absolute;
    top: 0;
    right: 0;
    font-size: 34rpx;
}

.release-news-btn {
    height: 80rpx;
    background-image: linear-gradient(#c71b28,#fd2631);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 80rpx;
    border-radius: 10rpx;
    margin: 36rpx 20rpx;
}

.modal {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    z-index: 9999999;
}

.show {
    display: block;
}

.hidden {
    display: none;
}

.modal_inf {
    width: 530rpx;
    height: 270rpx;
    background-color: #fff;
    border-radius: 16rpx;
    position: fixed;
    top: 400rpx;
    left: 120rpx;
    padding-top: 100rpx;
    text-align: center;
    font-size: 30rpx;
}

.modal_cross {
    font-size: 50rpx;
    font-weight: normal;
    position: absolute;
    top: 0rpx;
    left: 480rpx;
    width: 50rpx;
    height: 50rpx;
}

.release-yfk {
    font-size: 30rpx;
    text-align: center;
    width: 164rpx;
    margin: 0 auto;
    padding-bottom: 6rpx;
    color: #777474;
}

.goodthings-addr {
    display: flex;
    height: 60rpx;
    align-items: center;
}

.goodthings-addr image {
    width: 24rpx;
    height: 29rpx;
    margin-right: 10rpx;
}

.jftab01-modal-info-wrapper {
    width: 604rpx;
    background-color: #fff;
    margin: 520rpx auto;
    border-radius: 10rpx;
    padding: 54rpx 50rpx;
}

.jftab01-modal-info-cont {
    font-size: 32rpx;
    line-height: 40rpx;
    letter-spacing: 0.7px;
    text-align: justify;
}

.jftab01-modal-info-btn {
    height: 80rpx;
    background-color: #c71021;
    color: #fff;
    text-align: center;
    line-height: 80rpx;
    border-radius: 10rpx;
    font-size: 32rpx;
    margin-top: 40rpx;
}

.typeChoose {
    margin-top: 20rpx;
    padding: 22rpx;
    background: #fff;
    font-size: 30rpx;
    border-radius: 10rpx;
}

.titleType {
    color: #bababa;
    margin-bottom: 20rpx;
}

.typeWrap {
    display: flex;
    justify-content: space-between;
}

.typeItem {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.typeChooseIcon {
    border: 0.01rem solid #b5b5b5;
    width: 1rem;
    height: 1rem;
    border-radius: 50%;
    margin-right: 20rpx;
}

.isChoose {
    background: #ccc;
    width: 0.5rem;
    height: 0.5rem;
    border-radius: 50%;
    margin: 0.25rem;
}