.tabWrap {
    width: 100%;
    height: 80rpx;
    background: #fff;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 0 0 10rpx;
}

.tabItem {
    padding: 10rpx 20rpx;
    font-size: 28rpx;
    color: #333;
    border-bottom: 4rpx solid #fff;
}

.activeStyle {
    font-weight: bold;
    font-size: 30rpx;
    border-color: #e94a0b;
}

.wode-collect-wrapper {
    padding: 0rpx 20rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.collect-item {
    padding: 10rpx 0rpx;
    border-bottom: 1px solid #f5f5f5;
    overflow: hidden;
}

.collect-item-tit {
    font-size: 32rpx;
    width: 100%;
    padding: 10rpx 20rpx;
}

.collect-time {
    font-size: 25rpx;
    color: #666;
    display: flex;
    justify-content: space-between;
}

.content image {
    width: 35%;
    height: 100%;
    min-height: 218rpx;
}

.collect-type {
    color: #ffff;
    padding: 5rpx 10rpx;
    border-radius: 10rpx;
    background-color: #FF7F00;
    margin-right: 20rpx;
    font-size: 25rpx;
}

.isOutBtn {
    background: #ccc;
    color: #fff;
    width: max-content;
    padding: 0 20rpx;
    font-size: 25rpx;
    border-radius: 10rpx;
}

.typeStyle {
    border: 1rpx solid #fff;
    padding: 0 10rpx;
    font-size: 25rpx;
    border-radius: 5rpx;
    height: 35rpx;
    line-height: 35rpx;
    font-weight: normal;
    width: max-content;
    margin: 10rpx 0;
}

.container {
    margin: 20rpx;
    padding: 30rpx 20rpx;
    border-radius: 20rpx;
    background: #fff;
}

.touch-item {
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
    border: 1rpx solid #fafafa;
    width: 100%;
    overflow: hidden;
    margin-bottom: 30rpx;
    border-radius: 20rpx;
    box-shadow: 10rpx 10rpx 10rpx 5rpx #e3e3e3;
}

.content {
    width: 100%;
    line-height: 22px;
    margin-right: 0;
    transition: all 0.4s;
    transform: translateX(90px);
    margin-left: -90px;
}

.del {
    background-color: orangered;
    width: 90px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #fff;
    transform: translateX(90px);
    transition: all 0.4s;
}

.touch-move-active .content,.touch-move-active .del {
    transform: translateX(0);
}