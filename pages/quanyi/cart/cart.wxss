.qy-online-goods-wrapper {
    margin: 20rpx;
    background-color: #fff;
    display: flex;
    border-bottom: 1px solid #f0f0f0;
    padding: 30rpx 20rpx;
    border-radius: 10rpx;
}

.qy-goodsdel-changemodal-botsizeimg {
    display: flex;
    align-items: center;
    border: 1px solid #e3e3e3;
    border-radius: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.qy-online-goods-wrapper image {
    width: 200rpx;
    height: 200rpx;
    margin-right: 24rpx;
}

.qy-online-goods {
    width: 62%;
    margin-top: -6rpx;
}

.qy-online-goods-name {
    font-size: 32rpx;
    letter-spacing: 1rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    line-height: 46rpx;
    padding-bottom: 6rpx;
}

.qy-online-spec-name {
    background: #F9F9FB;
    color: #A9A8AB;
    font-size: 28rpx;
    padding: 6rpx 20rpx;
    width: max-content;
    border-radius: 5rpx;
}

.qy-online-goods-price {
    font-size: 34rpx;
    color: #C64C1C;
    display: flex;
    justify-content: space-between;
}

.qy-online-goods-size {
    font-size: 26rpx;
    color: #666;
    margin-top: 10rpx;
}

.qy-online-goods-wrapper image.minus,.qy-online-goods-wrapper image.add {
    width: 34rpx;
    height: 34rpx;
    border-radius: 6rpx;
    margin-right: 0rpx;
}

.one {
    padding: 0 10px;
    color: #999;
    font-size: 28rpx;
    border-left: 1px solid #e3e3e3;
    border-right: 1px solid #e3e3e3;
}

.qy-online-goods-wrapper image.qy-carted {
    width: 40rpx;
    height: 40rpx;
    margin-top: 80rpx;
}

.qy-fillorder-btn-wrapper {
    height: 100rpx;
    background-color: #fff;
    display: flex;
    align-items: center;
    position: fixed;
    bottom: 0rpx;
    width: 100%;
}

.qy-fillorder-btn {
    width: 260rpx;
    color: #fff;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}

.selectBtn {
    background-color: #c71021;
}

.noBtn {
    background-color: #aaa;
}

.qy-bot-all {
    width: 35%;
    padding-left: 30rpx;
}

.qy-bot-all image {
    width: 30rpx;
    height: 30rpx;
}

.qy-bot-all-tex {
    color: #999;
    font-size: 18rpx;
}

.chooseWay {
    background: #fff;
    padding: 20rpx;
    text-align: right;
    font-size: 27rpx;
    margin: 0 20rpx;
}

.computerIcon {
    padding: 0 15rpx;
    width: 50rpx;
    height: 50rpx;
    color: #bababa;
}