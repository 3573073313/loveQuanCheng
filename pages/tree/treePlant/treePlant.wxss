.page {
    background: #036d39;
    position: relative;
}

.navGoHome {
    position: fixed;
    left: 40rpx;
    z-index: 20;
}

.goHome {
    width: 72rpx;
    height: 60rpx;
}

.light {
    position: absolute;
    width: 100%;
}

.light image,.treeBtn {
    width: 100%;
}

.treeBtn {
    transition: transform 0.5s;
    transition: transform 0.5s,-webkit-transform 0.5s;
}

.topNav {
    position: absolute;
    height: 30%;
    top: 16%;
    width: 100%;
    text-align: center;
    z-index: 50;
}

.titleImg {
    width: 70%;
    margin: 0 auto 20rpx;
}

.titleImg image {
    width: 100%;
    height: 360rpx;
}

.joinCount {
    color: #fff;
    font-size: 34rpx;
}

.countStyle {
    color: #fff300;
    border: 1rpx solid #fff300;
    padding: 0 20rpx;
    border-radius: 20rpx;
    font-size: 30rpx;
}

.botBg {
    width: 100%;
    position: absolute;
    bottom: 0;
}

.botContent {
    position: absolute;
    bottom: 5%;
    width: 60%;
    left: 20%;
}

.detail {
    color: #fff;
    border-radius: 45rpx;
    background: #00ab38;
    padding: 10rpx 40rpx;
    margin: 10rpx auto 0;
    width: max-content;
    font-size: 28rpx;
}

.modal {
    background: #feebcc;
    border-radius: 45rpx;
    z-index: 100;
    position: fixed;
    top: 25%;
    left: 10%;
    width: 80%;
    min-height: 45%;
    padding: 5%;
    height: max-content;
    border-bottom: 10rpx solid #d38d62;
}

.iconClose {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 30rpx;
    right: 30rpx;
}

.centerImg {
    width: 40%;
    height: 100rpx;
    position: absolute;
    top: -50rpx;
    left: 30%;
}

.modalTitle {
    font-size: 40rpx;
    font-weight: bold;
    text-align: center;
    margin-top: 50rpx;
    color: #F6A23B;
}

.modalDetail {
    margin-top: 20rpx;
    line-height: 60rpx;
}

.ol {
    color: #F6A23B;
    font-weight: bold;
    margin-right: 20rpx;
}