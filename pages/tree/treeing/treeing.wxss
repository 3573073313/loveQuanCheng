.page {
    background: #b4eeff;
}

.goNavBack {
    position: fixed;
    left: 40rpx;
    padding: 20rpx;
}

.navBack {
    width: 20rpx;
    height: 20rpx;
    border-top: 4rpx solid #090909;
    border-left: 4rpx solid #090909;
    transform: rotate(-40deg);
}

.treeingBg {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 1000rpx;
}

.user {
    position: fixed;
    top: 10%;
    z-index: 10;
    left: 30rpx;
    display: flex;
    font-weight: bold;
    line-height: 100rpx;
}

.userImg {
    border-radius: 50%;
    overflow: hidden;
    width: 100rpx;
    height: 100rpx;
    margin-right: 30rpx;
}

.userImg image {
    width: 100%;
    height: 100%;
}

.flowingCloud {
    position: absolute;
    z-index: 3;
    right: 0;
    bottom: 0;
}

.flowingCloud1 {
    width: 40%;
    height: 40%;
    top: 12%;
    animation: cloud1 7s linear infinite;
}

.flowingCloud2 {
    width: 20%;
    height: 20%;
    top: 20%;
    animation: cloud2 9s linear infinite;
}

.flowingCloud3 {
    width: 30%;
    height: 30%;
    top: 25%;
    animation: cloud3 12s linear infinite;
}

.flowingCloud image,.treeGrow image,.startTreeing>image {
    width: 100%;
    height: 50rpx;
}

@-webkit-keyframes cloud1 {
    from {
        left: -40%;
    }

    to {
        left: 100%;
    }
}

@keyframes cloud1 {
    from {
        left: -40%;
    }

    to {
        left: 100%;
    }
}

@-webkit-keyframes cloud2 {
    from {
        left: -80%;
    }

    to {
        left: 100%;
    }
}

@keyframes cloud2 {
    from {
        left: -80%;
    }

    to {
        left: 100%;
    }
}

@-webkit-keyframes cloud3 {
    from {
        left: -140%;
    }

    to {
        left: 100%;
    }
}

@keyframes cloud3 {
    from {
        left: -140%;
    }

    to {
        left: 100%;
    }
}

.treeing {
    width: 100%;
    height: 70%;
    position: absolute;
    bottom: 150rpx;
    text-align: center;
}

.soil {
    position: absolute;
    bottom: 0;
    width: 34%;
    height: 30rpx;
    left: 33%;
}

.seedModal {
    width: 100%;
    height: calc(100% - 100rpx);
    display: flex;
    justify-content: center;
    align-items: center;
}

.seedWrap {
    height: calc(100% - 100rpx);
    width: 100%;
    position: relative;
}

.seedImg {
    width: 60%;
    height: 100rpx;
    margin: 40rpx auto 0;
    display: block;
}

.treeBtn {
    color: #fff;
    background: linear-gradient(#fd3633,#fe6d4c);
    border-radius: 20rpx;
    height: 80rpx;
    line-height: 60rpx;
    width: 80%;
    font-size: 35rpx;
    padding: 10rpx 40rpx;
    text-align: center;
    position: absolute;
    bottom: 70rpx;
    left: 10%;
}

.seedBubble {
    position: fixed;
    width: 26%;
    bottom: 50%;
    left: 37%;
    animation: seed 2.5s linear forwards;
}

@-webkit-keyframes seed {
    0% {
        bottom: 50%;
        transform: scale(1);
    }

    90% {
        transform: scale(0.2);
    }

    100% {
        transform: scale(0);
        bottom: 100rpx;
    }
}

@keyframes seed {
    0% {
        bottom: 50%;
        transform: scale(1);
    }

    90% {
        transform: scale(0.2);
    }

    100% {
        transform: scale(0);
        bottom: 100rpx;
    }
}

.seedBubble image {
    width: 100%;
}

.modal {
    background: #feebcc;
    border-radius: 45rpx;
    z-index: 10;
    position: fixed;
    top: 25%;
    left: 5%;
    width: 90%;
    height: 50%;
    border-bottom: 10rpx solid #d38d62;
}

.modalTitle {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx;
    height: 100rpx;
    border-bottom: 2rpx solid rgba(0,0,0,0.1);
}

.iconClose {
    width: 30rpx;
    height: 30rpx;
    padding: 20rpx;
}

.modalTitleImg {
    width: 85%;
    max-height: 100rpx;
}

.startTreeing {
    position: absolute;
    width: 46%;
    bottom: 20rpx;
    left: 27%;
}

.treeList {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    height: 780rpx;
    overflow: hidden;
}

.treeItem {
    width: 33%;
    text-align: center;
    margin-top: 50rpx;
    display: inline-block;
}

.treeList .treeItem:last-child {
    padding-bottom: 50rpx;
}

.treeImg {
    height: 120rpx;
    width: 120rpx;
    background-size: 100%;
    line-height: 166rpx;
    text-align: center;
    margin: 0 auto;
}

.treeImg image {
    width: 60%;
    height: 60%;
}

.treeName {
    font-weight: bold;
    font-size: 29rpx;
    margin: 10rpx 0;
}

.treeCount {
    font-size: 25rpx;
    color: #969a96;
}

.progressBot {
    width: 50%;
    height: 20rpx;
    background: #da6344;
    border-radius: 20rpx;
    position: fixed;
    bottom: 100rpx;
    left: 25%;
}

.progressBg {
    border-radius: 20rpx;
    width: 0%;
    height: 20rpx;
}

.progressBgStart {
    animation: progress 3s linear forwards;
}

@-webkit-keyframes progress {
    form {
        width: 0%;
    }

    to {
        width: 50%;
    }
}

@keyframes progress {
    form {
        width: 0%;
    }

    to {
        width: 50%;
    }
}

.treeGrow {
    position: absolute;
    bottom: -7%;
    width: 52%;
    left: 24%;
}

.treeGrowStart {
    animation: tree 3s linear forwards;
}

@-webkit-keyframes tree {
    from {
        transform: scale(0);
        bottom: -30%;
        opacity: 0;
    }

    to {
        transform: scale(0.5);
        bottom: -12%;
        opacity: 1;
    }
}

@keyframes tree {
    from {
        transform: scale(0);
        bottom: -30%;
        opacity: 0;
    }

    to {
        transform: scale(0.5);
        bottom: -12%;
        opacity: 1;
    }
}

.treeGrowEnd {
    transform: scale(0.5);
    animation: treeEnd 3s linear forwards;
}

@-webkit-keyframes treeEnd {
    from {
        transform: scale(0.5);
        bottom: -12%;
    }

    to {
        transform: scale(1);
        bottom: 3%;
    }
}

@keyframes treeEnd {
    from {
        transform: scale(0.5);
        bottom: -12%;
    }

    to {
        transform: scale(1);
        bottom: 3%;
    }
}

.progressBgEnd {
    animation: progressEnd 3s linear forwards;
    width: 50%;
}

.fruitWrap {
    position: absolute;
    top: 100rpx;
    left: 0;
    display: flex;
}

.fruit {
    width: 110rpx;
    position: relative;
    font-size: 25rpx;
}

.fruitWrap .fruit:nth-of-type(1) {
    margin-top: 10%;
    margin-left: 0;
}

.fruitWrap .fruit:nth-of-type(2) {
    margin-top: -70rpx;
    margin-left: -13%;
}

.fruitWrap .fruit:nth-of-type(3) {
    margin-top: 70rpx;
    margin-left: -6%;
}

.fruitWrap .fruit:nth-of-type(4) {
    margin-top: -120rpx;
    margin-left: -8%;
}

.fruitWrap .fruit:nth-of-type(5) {
    margin-left: 3%;
    margin-top: 13%;
}

.fruitWrap .fruit:nth-of-type(6) {
    margin-top: -80rpx;
    margin-left: -30%;
}

.fruitWrap .fruit1 {
    animation: fruit1 5s linear forwards;
}

.fruitWrap .fruit2 {
    animation: fruit2 5s linear forwards;
}

.fruitWrap .fruit3 {
    animation: fruit3 5s linear forwards;
}

@-webkit-keyframes fruit1 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.7);
    }

    50% {
        transform: rotate(-20deg) scale(0.7);
    }

    75% {
        transform: rotate(20deg) scale(0.7);
    }

    100% {
        transform: rotate(0deg) scale(0.7);
    }
}

@keyframes fruit1 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.7);
    }

    50% {
        transform: rotate(-20deg) scale(0.7);
    }

    75% {
        transform: rotate(20deg) scale(0.7);
    }

    100% {
        transform: rotate(0deg) scale(0.7);
    }
}

@-webkit-keyframes fruit2 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.6);
    }

    50% {
        transform: rotate(-20deg) scale(0.6);
    }

    75% {
        transform: rotate(20deg) scale(0.6);
    }

    100% {
        transform: rotate(0deg) scale(0.6);
    }
}

@keyframes fruit2 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.6);
    }

    50% {
        transform: rotate(-20deg) scale(0.6);
    }

    75% {
        transform: rotate(20deg) scale(0.6);
    }

    100% {
        transform: rotate(0deg) scale(0.6);
    }
}

@-webkit-keyframes fruit3 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.5);
    }

    50% {
        transform: rotate(-20deg) scale(0.5);
    }

    75% {
        transform: rotate(20deg) scale(0.5);
    }

    100% {
        transform: rotate(0deg) scale(0.5);
    }
}

@keyframes fruit3 {
    0% {
        transform: scale(0);
    }

    25% {
        transform: scale(0.5);
    }

    50% {
        transform: rotate(-20deg) scale(0.5);
    }

    75% {
        transform: rotate(20deg) scale(0.5);
    }

    100% {
        transform: rotate(0deg) scale(0.5);
    }
}

.fruitName {
    position: absolute;
    top: 36rpx;
    left: 15rpx;
    color: #fff;
    font-weight: bold;
}

@-webkit-keyframes progressEnd {
    form {
        width: 50%;
    }

    to {
        width: 100%;
    }
}

@keyframes progressEnd {
    form {
        width: 50%;
    }

    to {
        width: 100%;
    }
}

.shake {
    animation: shakePot 1s 1s linear;
}

@-webkit-keyframes shakePot {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(0.7);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes shakePot {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(0.7);
    }

    100% {
        transform: scale(1);
    }
}

.watering {
    position: fixed;
    right: 120rpx;
    top: 60%;
    width: 200rpx;
    text-align: right;
}

.show {
    display: block;
}

.hidden {
    display: none;
}

.wateringCan {
    width: 200rpx;
    height: 100rpx;
}

.isWateringStart {
    transform: rotate(0deg) translate(0);
    animation: waterStart 4s linear forwards;
}

@-webkit-keyframes waterStart {
    0% {
        transform: rotate(0deg) translate(0);
    }

    20% {
        transform: rotate(-40deg) translate(100rpx,-100rpx);
    }

    80% {
        transform: rotate(-40deg) translate(100rpx,-100rpx);
    }

    100% {
        transform: rotate(0deg) translate(0);
    }
}

@keyframes waterStart {
    0% {
        transform: rotate(0deg) translate(0);
    }

    20% {
        transform: rotate(-40deg) translate(100rpx,-100rpx);
    }

    80% {
        transform: rotate(-40deg) translate(100rpx,-100rpx);
    }

    100% {
        transform: rotate(0deg) translate(0);
    }
}

.wateringBtn {
    background: linear-gradient(#008ffe,#00c1ff);
    color: #fff;
    border: 2rpx solid #fff;
    font-size: 25rpx;
    height: 45rpx;
    line-height: 40rpx;
    width: max-content;
    padding: 0 20rpx;
    border-radius: 20rpx;
    margin-top: 5rpx;
    right: -30rpx;
}

.waterMore {
    width: 108rpx;
    height: 60rpx;
    transform: rotate(-40deg) translate(20rpx,-150rpx);
}

.waterMoreImg {
    width: 108rpx;
    height: 60rpx;
}

.isWaterMore {
    animation: waterMoreItem 1s linear 2.5;
    transform: translateX(30rpx);
    opacity: 0;
}

@-webkit-keyframes waterMoreItem {
    0% {
        transform: translateX(30rpx);
        opacity: 1;
    }

    100% {
        transform: translateX(-30rpx);
        display: block;
        opacity: 0;
    }
}

@keyframes waterMoreItem {
    0% {
        transform: translateX(30rpx);
        opacity: 1;
    }

    100% {
        transform: translateX(-30rpx);
        display: block;
        opacity: 0;
    }
}

.finishModal {
    width: 70%;
    min-height: 35%;
    position: fixed;
    top: 33%;
    left: 15%;
    background: #f9f0d9;
    border: 4rpx solid #42799b;
    border-radius: 45rpx;
    z-index: 100;
    text-align: center;
    color: #da6142;
    animation: finish 0.5s linear;
}

@-webkit-keyframes finish {
    from {
        transform: scale(0);
    }

    to {
        transform: scale(1);
    }
}

@keyframes finish {
    from {
        transform: scale(0);
    }

    to {
        transform: scale(1);
    }
}

.finishTitle {
    position: absolute;
    width: 120%;
    top: -18%;
    left: -10%;
}

.finishHead {
    margin-top: 20%;
    font-size: 60rpx;
    margin-bottom: 30rpx;
    font-weight: bold;
}

.finishContent {
    font-size: 35rpx;
    line-height: 80rpx;
    margin-bottom: 20rpx;
}

.finishNum {
    font-size: 50rpx;
    font-weight: bold;
}

.getBtn {
    margin: 0 auto 25rpx;
    background: linear-gradient(#fd3633,#fe6d4c);
    color: #fff;
    width: max-content;
    border-radius: 45rpx;
    height: 70rpx;
    line-height: 70rpx;
    padding: 0 40rpx;
}